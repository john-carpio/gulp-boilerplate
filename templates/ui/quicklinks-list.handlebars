{{#each quickLinks }}
    <li{{#ifCond this.type '===' 'disabled'}} class="disabled"{{/ifCond}}>
        <a class="nav-link level-{{../level}}" data-coupon="{{isCoupon}}" data-url="{{this.url}}">
            <span class="tree-name">{{this.description}}</span>
            {{#if showCount}} 
                <span class="tree-number">{{items.length}}</span>
            {{/if}}
            {{#ifCond this.type '===' 'section'}}
                <span class="tree-action"><i class="fa fa-plus"></i></span>
            {{/ifCond}}
        </a>

        {{#ifCond this.type '===' 'section'}}
            <ul class="nav-tree ">
                {{> ui/quicklinks-list quickLinks=this.items level=(add1 ../../level)}}
            </ul>
        {{/ifCond}}
    </li>
{{else}}
{{!--     <li>
        <a class="nav-link " >
            <span class="tree-name">{{t 'ui.loadingError'}}</span>
            
        </a>
    </li> --}}

{{/each}}