                        {{/each}}
                    {{/each}}

                    {{#each (getVTennMarket 'Total number of games in set 1 (9.5) 0' this.children) }}
                        {{#each this.children}}
                        {{!-- {{#foreach markets}} --}}
                            <div class="market-container">
                                <div class="european-row market-list {{toLowerCase this.description}}" >
                                    <div class="market-description">
                                        {{this.description}} - {{this.period}}
                                    </div>
                                    
                                        {{> virtual/event-block style=this.style children=this.children first=$first id=this.id eventID=../../event.id flags=this.flags}}
                                    
                                </div>
                            </div>
                        {{!-- {{/foreach}} --}}
                        {{/each}}
                    {{/each}}

                    {{#each (getVTennMarket 'Total number of games 0' this.children) }}
                        {{#each this.children}}
                        {{!-- {{#foreach markets}} --}}
                            <div class="market-container">
                                <div class="european-row market-list {{toLowerCase this.description}}" >
                                    <div class="market-description">
                                        {{this.description}} - {{this.period}}
                                    </div>
                                    
                                        {{> virtual/event-block style=this.style children=this.children first=$first id=this.id eventID=../../event.id flags=this.flags}}
                                    
                                </div>
                            </div>
                        {{!-- {{/foreach}} --}}
                        {{/each}}
                    {{/each}}

                
            </div>
        </div>
        {{#ifCond ../../round '>=' 3}}
            <div class="event-container {{style}} round-{{../../../round}} place-holder default-market-{{size this.children}} {{../../leagueClass}}" data-style="{{style}}">
                <div class=" m-{{id}} {{showFlags flags}} ">
                    
                    
                    <div class="opponent-container"> </div>
                        
                    <div class="market-container">
                        <div class="european-row market-list " >
                            
                        </div>
                    </div>

                    {{#each (getVTennMarket 'Total number of games in set 1 (10.5) 0' ../this.children) }}
                        {{#each this.children}}
                        {{!-- {{#foreach markets}} --}}
                            <div class="market-container">
                                <div class="european-row market-list {{toLowerCase this.description}}" >
                                    <div class="market-description">
                                        {{this.description}} - {{this.period}}
                                    </div>
                                    
                                        {{> virtual/event-block style=this.style children=this.children first=$first id=this.id eventID=../../event.id flags=this.flags}}
                                    
                                </div>
                            </div>
                        {{!-- {{/foreach}} --}}
                        {{/each}}
                    {{/each}}

                    <div class="market-container">
                        <div class="european-row market-list " >
                            
                        </div>
                    </div>

                    
                </div>
            </div>
            <div class="event-container {{style}} round-{{../../../round}} place-holder default-market-{{size this.children}} {{../../leagueClass}}" data-style="{{style}}">
                <div class=" m-{{id}} {{showFlags flags}} ">
                    
                    
                    <div class="opponent-container"> </div>
                        
                        <div class="market-container">
                            <div class="european-row market-list " >
                                
                            </div>
                        </div>

                        {{#each (getVTennMarket 'Total number of games in set 1 (8.5) 0' this.children) }}
                            {{#each this.children}}
                            {{!-- {{#foreach markets}} --}}
                                <div class="market-container">
                                    <div class="european-row market-list {{toLowerCase this.description}}" >
                                        <div class="market-description">
                                            {{this.description}} - {{this.period}}
                                        </div>
                                        
                                            {{> virtual/event-block style=this.style children=this.children first=$first id=this.id eventID=../../event.id flags=this.flags}}
                                        
                                    </div>
                                </div>
                            {{!-- {{/foreach}} --}}
                            {{/each}}
                        {{/each}}

                        <div class="market-container">
                            <div class="european-row market-list " >
                                
                            </div>
                        </div>

                    
                </div>
            </div>
        {{/ifCond}}
    {{/each}}
{{/ifCond}}

